.main
    padding-bottom 50px

.menu
    display flex
    align-items center

    a
        display flex
        align-items center
        width 100%
        height 100%

    img
        margin-right 10px
        width 14px
        height 14px

aside[class~='ant-layout-sider-collapsed']
    .menu
        display block

        span
            display none

div[class~='ant-tooltip-inner']
    img
        display none
