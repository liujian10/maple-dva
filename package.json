        "noform": "^1.3.38",
        "prop-types": "^15.7.2",
        "rc-queue-anim": "^1.8.2",
        "react": "^16.9.0",
        "react-dom": "^16.9.0"
    },
    "devDependencies": {
        "antd": "^3.23.2",
        "babel-plugin-dva-hmr": "^0.4.2",
        "babel-plugin-import": "^1.12.1",
        "babel-plugin-wrapper": "^1.0.17",
        "copy-webpack-plugin": "^5.0.4",
        "cross-env": "^5.2.1",
        "eslint": "^5.16.0",
        "eslint-config-umi": "^1.5.0",
        "eslint-plugin-flowtype": "^4.3.0",
