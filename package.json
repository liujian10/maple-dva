        "noform": "^1.1.3",
        "prop-types": "^15.6.2",
        "rc-queue-anim": "^1.6.8",
        "react": "^16.9.0",
        "react-dom": "^16.9.0"
    },
    "devDependencies": {
        "antd": "^3.6.6",
        "babel-plugin-dva-hmr": "^0.3.2",
        "babel-plugin-import": "^1.8.0",
        "babel-plugin-wrapper": "^1.0.17",
        "copy-webpack-plugin": "^4.5.2",
        "cross-env": "^5.2.0",
        "eslint": "^4.14.0",
        "eslint-config-umi": "^0.1.1",
        "eslint-plugin-flowtype": "^2.34.1",
