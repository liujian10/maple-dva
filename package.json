{
  "name": "maple-dva",
  "version": "1.0.1",
  "description": "A dva scaffold",
  "main": "dist",
  "scripts": {
    "start": "cross-env PORT=3000 BROWSER=none roadhog server",
    "build": "cross-env roadhog build",
    "lint": "eslint src/**/*.js",
    "fixeslint": "eslint src/**/*.js --fix",
    "stylint": "stylint src/**/*.styl",
    "pret": "prettier --single-quote --tab-width 4 --no-semi --trailing-comma all --write \"src/**/*.js\""
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint"
    ],
    "src/**/*.styl": [
      "stylint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@antv/data-set": "^0.10.2",
    "@hot-loader/react-dom": "^16.9.0",
    "bizcharts": "^3.5.5",
    "dva": "^2.4.0",
    "dva-loading": "^2.0.3",
    "history": "^4.9.0",
    "lodash": "^4.17.19",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-hot-loader": "^4.12.12",
    "react-router-config": "^5.0.1"
  },
  "devDependencies": {
    "antd": "^3.23.2",
    "babel-plugin-import": "^1.12.1",
    "babel-plugin-wrapper": "^1.0.17",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^5.2.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-umi": "^1.5.0",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "install": "^0.13.0",
    "husky": "^3.0.5",
    "json-loader": "^0.5.7",
    "lint-staged": "^9.2.5",
    "path": "^0.12.7",
    "postcss-pxtorem": "^4.0.1",
    "redbox-react": "^1.4.3",
    "roadhog": "2.5.0-beta.4",
    "stylint": "^2.0.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "yaml-loader": "^0.5.0"
  }
