        "eslint-plugin-react-hooks": "^2.0.1",
        "husky": "^3.0.5",
        "json-loader": "^0.5.7",
        "lint-staged": "^9.2.5",
        "path": "^0.12.7",
        "postcss-pxtorem": "^4.0.1",
        "redbox-react": "^1.4.3",
        "roadhog": "2.5.0-beta.4",
        "stylint": "^2.0.0",
        "stylus": "^0.54.7",
        "stylus-loader": "^3.0.2",
        "yaml-loader": "^0.5.0"
    }
