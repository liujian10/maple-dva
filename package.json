        "husky": "^0.12.0",
        "json-loader": "^0.5.7",
        "lint-staged": "^7.2.0",
        "path": "^0.12.7",
        "postcss-pxtorem": "^4.0.1",
        "redbox-react": "^1.4.3",
        "roadhog": "^2.4.5",
        "stylint": "^1.5.9",
        "stylus": "^0.54.5",
        "stylus-loader": "^3.0.2",
        "yaml-loader": "^0.5.0"
    }
